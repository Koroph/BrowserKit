{
    "name": "kh/browserkit",
    "description": "\"get user browser information\"",
    "type": "libraray",
    "require": {
        "php": "^7.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.3.3"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Koroph",
            "homepage": "http://koroph.site",
            "email": "yjkof1596@gmail.com"
        }
